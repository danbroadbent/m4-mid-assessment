<h2>Submit Link</h2>
<%= form_for(@link) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.submit "Submit" %>
<% end %>

<h2>Your links</h2>
<% @links.each do |link| %>
<div class="links">
  <%= link.title %></br>
  <%= link.url %>
</div></br>
<% end %>
